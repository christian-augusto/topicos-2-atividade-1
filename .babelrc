{
  "presets": [
    "@babel/preset-env",
    "@vue/cli-plugin-babel/preset",
    "minify"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@": "./src",
          "@components": "./src/components",
          "@errors": "./src/errors",
          "@pages": "./src/pages",
          "@styles": "./src/styles",
          "@assets": "./src/assets",
          "@interfaces": "./src/interfaces",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
